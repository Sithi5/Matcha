{% extends 'navbar/layout.html.twig' %}


{% block navItemLeft %}
	{#admin part#}
	{{ include('navbar/nav-item/admin.html.twig') }}
{% endblock %}
{% block navItemRight %}
	{% if not is_granted('IS_AUTHENTICATED_REMEMBERED') %}
		<li class="nav-item">
			<a class="btn nav-btn-sign-in" onclick="showModalLogin()">Sign in</a>
		</li>
	{% else %}
		<li class="nav-item">
			{{ include('navbar/dropdown/user-card-dropdown.html.twig') }}
		</li>
	{% endif %}
{% endblock %}